{
    "name": "@repo/logger",
    "version": "1.0.0",
    "private": true,
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "scripts": {
        "build": "npm run clean && tsup && tsc-alias",
        "dev": "tsup --watch",
        "clean": "rimraf dist",
        "clear:force": "rimraf *.js --force"
    },
    "dependencies": {
        "winston": "^3.10.0",
        "winston-loki": "^6.0.8",
        "dotenv": "^16.3.1",
        "zod": "^3.24.2"
    },
    "devDependencies": {
        "@repo/typescript-config": "*",
        "@types/node": "^20.0.0",
        "typescript": "^5.0.0",
        "eslint": "latest",
        "rimraf": "^6.0.1",
        "tsc-alias": "^1.8.10",
        "tsup": "^8.2.4",
        "ts-node": "^10.9.2"
    },
    "publishConfig": {
        "access": "restricted"
    }
}